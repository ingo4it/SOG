			var edge_class = '';
			var main_class = '';

			if (products_per_row == 2) {
				main_class = 'thumbnail col-sm-6 columns ';
			} else if (products_per_row == 3) {
				main_class = 'thumbnail col-sm-4 columns ';
			} else if (products_per_row == 4) {
				main_class = 'thumbnail col-sm-3 columns ';

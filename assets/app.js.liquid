  relatedProducts: function(){
  	if (window.testProducts.length > 0) {
  		$('.related-products').show();
  		var design_code = window.selected_product.variants[0].sku.split('-')[0];
  		var vendor = window.selected_product.vendor;
  		$.ajax({
				method: "GET",
				url: "https://search.sonsofgotham.com/related_products",
				data: { design_code: design_code, vendor: vendor }
			})
			.done(function( products ) {

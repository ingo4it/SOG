    		$(this).show();
    	} else if (content == 'women' && window.women_styles.indexOf(style) > -1) {
    		$(this).show();
    	} else if (content == 'kids' && window.kids_styles.indexOf(style) > -1) {
    		$(this).show();
    	} else if (content == 'baby' && window.baby_styles.indexOf(style) > -1) {
    		$(this).show();
    	} else if (content == 'goods' && window.goods_styles.indexOf(style) > -1) {

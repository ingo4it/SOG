			if (align_height) {
				maxWidth = product['image']['width'] / product['image']['height'] * collection_height;
			}
			var align_height_value = (product['image']['width'] >= product['image']['height'] ? 'width: 100%; height: auto;' : 'width: 100%;');
			html += '<div class="image__container" style=" max-width: ' + maxWidth + 'px; ">';
			var image1 = (product['image']['src'] ? product['image']['src'].replace(/\.(?=[^.]*$)/, "_x500.") : no_image.replace(/\.(?=[^.]*$)/, "_x500."));
			html += '<img src="' + image1 + '" alt="' + product['image']['alt'] + '" class="lazyload {{ settings.image_loading_style }}" style="' + align_height_value + ' max-width: ' + product['image']['width'] + 'px;" data-sizes="auto" data-src="' + image1 + '" />';
			html += '</div>';
			if (secondary_enable) {
				var image2 = (product['images'].length > 1 ? product['images'][1]['src'].replace(/\.(?=[^.]*$)/, "_x500.") : image1);
				html += '<div class="image__container" style="max-width: ' + (product['images'].length > 1 ? product['images'][1]['width'] : product['image']['width']) + 'px">';
				html += '<img src="' + image2 + '" class="secondary lazyload" alt="' + (product['images'].length > 1 ? product['images'][1]['alt'] : product['image']['alt']) + '" />';
        html += '</div>';
			}
			html += '</a>';
 			var price = (product['variants'][0] != null ? product['variants'][0]['price'] : '0');
 			// Product Details
 			html += '<div class="thumbnail-overlay">';
 			html += '<a href="/products/' + product['handle'] + '" itemprop="url" class="hidden-product-link">' + product['title'] + '</a>';
 			html += '<div class="info">';
 			html += '<div class="product-details"><span class="title" itemprop="name">' + product['title'] + '</span><span class="price "><small><em>from</em></small><span class="money"> $' + price + '</span></span></div>';
 			html += '</div>';
 			html += '</div>';
 			html += '<div class="banner_holder"></div>';
 			html += '</div>';

  		html += '<a class="product-info__caption hidden" href="/products/' + product.handle + '" itemprop="url">';
    	html += '<div class="product-details"><span class="title" itemprop="name">' + product['title'] + '</span> <span class="price"><small><em>from</em></small><span class="money">$' + price + '</span></span></div>';
    	html += '</a>';

			html += '</div>';
			html += '</div>';
			if (i > 0 && i % (products_per_row == products_per_row - 1)) {
				html += '<br class="clear product_clear">';
			}
		}

		$container.html(html);
		var $productWrap = $('.related_products .product-wrap');

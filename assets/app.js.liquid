  initOptionActions: function(product) {
  	/*----------  Make Cart Button Active When All options are selected  ----------*/
		var options_length = product.options.length;
		for (var i = 0; i < options_length; i++) {
			if ($('[name="option-' + i +'"]').length > 1) {
				$('[name="option-' + i +'"]').prop("checked", false);
				$('[name="option-' + i +'"]').removeAttr("checked");
			} else {
				$('[name="option-' + i +'"]').prop("checked", true);
			}
		}

		productPage.checkOptions(product);

		$("button.action_button").on('click', function(e) {
			if ($(this).hasClass("cart_disable")) {
				e.preventDefault();
			}
		});

		$('.variant-option').on('click', function() {
			productPage.checkOptions(window.selected_product);
		});
		/*----------  End - Make Cart Button Active  ----------*/ 
  },
  checkOptions: function(product) {
  	var options_length = product.options.length;
		var enable = true;
		for (var i = 0; i < options_length; i++) {
			if ($('[name="option-' + i +'"]:checked').length == 0) {
				enable = false;
				break;
			}
		}

		if (!enable) {
			$(".action_button").removeClass("add_to_cart");
			$(".action_button").addClass("cart_disable");
		} else {
			$(".action_button").addClass("add_to_cart");
			$(".action_button").removeClass("cart_disable");
		}
  },
  initSizeChart: function(product) {
  	var tags = product.tags.split(", ");
  	var tag = '';
  	for (var i = 0; i < tags.length; i++) {
  		var t = tags[i].toLowerCase();
  		if (t.includes('_size_chart')) {
  			tag = t;
  			break;
  		}
  	}

  	var chart_data = window.chart_data[tag];

  	if (tag != '' && (typeof chart_data != "undefined")) {
  		var option_index = -1;
  		for (var i = 0; i < product.options.length; i++) {
  			if (product.options[i]['name'].toLowerCase() == "size") {
  				option_index = i;
  				break;
  			}
  		}

  		if (option_index > -1 && product.variants.length > 0) {
  			$('#modal-size-chart .modal-size-chart__subhead').text(product.variants[0]['option' + option_index.toString()]);
  		} else {
  			$('#modal-size-chart .modal-size-chart__subhead').text(tag.heading);
  		}
  		
  		var html = '';

  		if (chart_data['image'] != null) {
  			html += '<div class="col-sm-4">';
  			html += '<img src="' + chart_data['image'] + '" alt="" class="img-responsive modal-size-chart__image">';
  			html += '</div>';
  		}

  		html += '<div class="' + (chart_data['image'] == null ? 'col-sm-12' : 'col-sm-8') + '">';
      html += '<div class="table-responsive">';
      html += '<table class="table modal-size-chart__table">';
      html += '<thead><tr>';
      if (chart_data['chart_nosize']) {
      	html += '<th style="text-align: center">Size</th>';	
      }
      html += '<th style="text-align: center">Width</th>';
      html += '<th>Length</th>';
      html += '</tr></thead>';
      
      html += '<tbody>';
      var tbl_rows = chart_data['table'].split(",");
      for (var i = 0; i < tbl_rows.length; i++) {
      	var cells = tbl_rows[i].split(" ");
      	html += '<tr>';
      	if (chart_data['chart_nosize']) {
      		html += '<td align="center"><strong>' + cells[0] + '</strong></td>';
      		html += '<td align="center"><strong>' + cells[1] + '</strong></td>';
      		html += '<td align="center"><strong>' + cells[2] + '</strong></td>';
      	} else {
      		html += '<td align="center"><strong>' + cells[0] + '</strong></td>';
      		html += '<td align="center"><strong>' + cells[1] + '</strong></td>';
      	}
      	html += '</tr>';
      }
      html += '</tbody>';
      html += '</table>';
      html += '</div>';
      html += '</div>';

      $('#modal-size-chart .modal-size-chart__chart').html(html);
  	}
  },
